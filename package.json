{
  "name": "shiqingqi.com",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "22.x",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd",
  "imports": {
    "#src/*": "./src/*"
  },
  "scripts": {
    "predev": "rimraf .next && rimraf public/sw.js",
    "prebuild": "rimraf .next && rimraf public/sw.js",
    "codegen": "npm-run-all codegen:*",
    "codegen:openapi": "openapi-typescript https://developer.themoviedb.org/openapi/tmdb-api.json -o src/_generated/tmdbV3.d.ts && prettier 'src/_generated/tmdbV3.d.ts' -w",
    "codegen:zod": "tsx tooling/tmdb-codegen/generate-selective-zod.ts && prettier 'src/_generated/tmdb-zod.ts' -w",
    "codegen:tmdb": "node tooling/tmdb-codegen/generator.js && prettier 'src/_generated/*.ts' -w",
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:tsc": "tsc --noEmit",
    "start": "next start",
    "lint": "eslint .",
    "lint:changed": "(git diff --name-only --diff-filter=d HEAD; git ls-files --others --exclude-standard) | grep -E '\\.(js|jsx|ts|tsx)$' | sort -u | xargs -r eslint --fix",
    "format:write": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,md,mjs}' --ignore-path .gitignore",
    "format:check": "prettier --check '**/*.{js,jsx,ts,tsx,json,css,md,mjs}' --ignore-path .gitignore",
    "format:changed": "git diff --name-only --diff-filter=d HEAD | grep -E '\\.(js|jsx|ts|tsx|json|css|md|mjs)$' | xargs -r prettier --write",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@mui/utils": "^7.3.6",
    "@phosphor-icons/react": "^2.1.10",
    "@serwist/next": "^9.2.3",
    "@stylexjs/stylex": "^0.17.0",
    "@tanstack/react-query": "5.90.11",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "1.2.0",
    "next": "16.0.10",
    "openai": "^6.1.0",
    "react": "19.3.0-canary-52684925-20251110",
    "react-dom": "19.3.0-canary-52684925-20251110",
    "react-error-boundary": "^6.0.0",
    "react-remove-scroll": "^2.7.1",
    "react-virtuoso": "^4.17.0",
    "serwist": "10.0.0-preview.14",
    "sharp": "^0.34.5",
    "twgl.js": "^7.0.0",
    "zod": "3",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@babel/parser": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "@eslint/compat": "^2.0.0",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@next/eslint-plugin-next": "16.0.10",
    "@playwright/test": "^1.56.1",
    "@serwist/turbopack": "10.0.0-preview.14",
    "@stylexjs/babel-plugin": "^0.17.0",
    "@stylexjs/eslint-plugin": "^0.17.0",
    "@stylexjs/postcss-plugin": "^0.17.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/babel__core": "^7.20.5",
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^24.10.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-react-compiler": "19.0.0-beta-714736e-20250131",
    "esbuild-wasm": "^0.25.12",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.10",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "jsdom": "^27.2.0",
    "modern-normalize": "^3.0.1",
    "next-i18n-router": "^5.5.5",
    "npm-run-all2": "^8.0.4",
    "openapi-typescript": "^7.10.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^16.1.1",
    "postcss-preset-env": "^10.5.0",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite-plugin-babel": "^1.3.2",
    "vitest": "^4.0.16"
  }
}
